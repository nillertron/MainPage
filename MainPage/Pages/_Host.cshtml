@page "/"
@namespace MainPage.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="keywords" content="Freelance, web design, programmer, html, asp, asp.net, .net, dot net, hire, developer, full stack, responsive" />
    <meta name="description" content="I am a freelance full stack developer, ready to help you!" />
    <title>Nichlas Christensen Portfolio</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/MyPracticeStyles.css" />
    <link rel="icon" href="/css/img/web_hi_res_512.png" />
</head>
<body>
    <app>
        <component type="typeof(App)" render-mode="ServerPrerendered" />
    </app>

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>
    <script type="text/javascript" src="~/JS/JQuery.js"></script>
    <script type="text/javascript" src="~/JS/bootstrap.bundle.min.js"></script>
    <script type="text/javascript" src="~/js/Library.js"></script>
    <script src="_framework/blazor.server.js"></script>
</body>
</html>
